{
  "variables": {
    "apiKey": "#{ENV.FUSIONAUTH_API_KEY}",
    "adminUserEmail": "#{ENV.ADMIN_USER_EMAIL}",
    "adminUserPassword": "#{ENV.ADMIN_USER_PASSWORD}",
    "clientId": "#{ENV.FUSIONAUTH_CLIENT_ID}",
    "clientSecret": "#{ENV.FUSIONAUTH_CLIENT_SECRET}"
  },
  "apiKeys": [
    {
      "key": "#{apiKey}"
    }
  ],
  "requests": [
    {
      "method": "POST",
      "url": "/api/user/registration",
      "body": {
        "user": {
          "email": "#{adminUserEmail}",
          "password": "#{adminUserPassword}"
        },
        "registration": {
          "applicationId": "#{FUSIONAUTH_APPLICATION_ID}",
          "roles": ["admin"]
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/application/#{clientId}",
      "body": {
        "application": {
          "name": "test_app",
          "oauthConfiguration": {
            "clientSecret": "#{clientSecret}"
          }
        }
      }
    }
  ]
}
