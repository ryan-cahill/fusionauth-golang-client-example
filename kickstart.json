        "user": {
          "email": "#{adminUserEmail}",
          "password": "#{adminUserPassword}"
        },
        "registration": {
          "applicationId": "#{FUSIONAUTH_APPLICATION_ID}",
          "roles": ["admin"]
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/application/8cc4c901-1852-4e62-a655-699f2c94ffdc",
      "body": {
        "application": {
          "name": "test_app",
          "oauthConfiguration": {
            "clientSecret": "OshhDO0d1Y6Hrejsd697Yo4unTar_gI00fHiIASMZGc"
          }
        }
      }
    }
  ]
}
